{
  "dependencies": {
    "ws": "^8.16.0"
  },
  "description": "Deepgram STT provider using nova-3 model",
  "devDependencies": {
    "@biomejs/biome": "^2.4.3",
    "@types/node": "^25.2.3",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.0"
  },
  "main": "index.js",
  "name": "@wopr-network/wopr-plugin-voice-deepgram-stt",
  "scripts": {
    "build": "tsc",
    "check": "biome check src/ && tsc --noEmit",
    "dev": "tsc -w",
    "format": "biome format --write src/",
    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "version": "1.0.0",
  "wopr": {
    "plugin": {
      "category": "voice",
      "description": "Cloud STT using Deepgram's nova-3 model",
      "install": [],
      "name": "voice-deepgram-stt",
      "requires": {
        "env": [
          "DEEPGRAM_API_KEY"
        ]
      },
      "type": "stt"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "release": {
    "extends": "@wopr-network/semantic-release-config"
  }
}
